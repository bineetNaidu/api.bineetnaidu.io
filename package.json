{
  "name": "api.bineetnaidu.io",
  "version": "2.0.0",
  "description": "A Node js server for my API's.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bineetNaidu/api.bineetnaidu.io.git"
  },
  "author": {
    "name": "Bineet Naidu",
    "url": "https://bineetnaidu.github.io",
    "email": "bineetnaiduapps@gmail.com"
  },
  "license": "GNU AGPLv3",
  "bugs": {
    "url": "https://github.com/bineetNaidu/api.bineetnaidu.io/issues"
  },
  "homepage": "https://github.com/bineetNaidu/api.bineetnaidu.io#readme",
  "scripts": {
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run build",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.4",
    "@nestjs/common": "^8.3.1",
    "@nestjs/core": "^8.3.1",
    "@nestjs/graphql": "^10.0.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/platform-express": "^8.3.1",
    "apollo-server-express": "^3.6.3",
    "argon2": "^0.28.4",
    "buffer-to-stream": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^1.28.1",
    "dotenv": "^16.0.0",
    "graphql": "^16.3.0",
    "helmet": "^5.0.2",
    "mongoose": "^6.2.2",
    "multer-storage-cloudinary": "^4.0.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.1",
    "@nestjs/schematics": "^8.0.6",
    "@nestjs/testing": "^8.3.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.18",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
